{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 22",
	"_version": "22.0.0",
	"compilerOptions": {
		"rootDir": "./src",
		"outDir": "./dist",
		"module": "nodenext",
		"target": "es2022",
		"moduleResolution": "node16",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"include": ["src"]
}
